{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug gatsby develop",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "args": ["develop"],
      "smartStep": true,
      "stopOnEntry": false,
      "runtimeArgs": ["--nolazy"],
      "sourceMaps": false
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug gatsby build",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "args": ["build"],
      "smartStep": true,
      "stopOnEntry": false,
      "runtimeArgs": ["--nolazy"],
      "sourceMaps": false
    }
  ]
}
